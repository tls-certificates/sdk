{
    "name": "tls-certificates/sdk",
    "description": "The php sdk for TLS Certificates.",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": ">4.0"
    },
    "autoload": {
        "psr-4": {
            "TlsCertificates\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TlsCertificates\\Sdk\\Test\\": "tests/"
        }
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": ">=5.0",
        "illuminate/support": "^8.51",
        "illuminate/validation": "^8.51"
    }
}
